{
  "name": "gdb-js",
  "version": "0.0.1",
  "description": "JavaScript interface for GDB.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "test": "mocha --require source-map-support/register --compilers js:babel-core/register test/*.js",
    "build": "babel src/ -d lib --source-maps --copy-files",
    "watch": "npm run build -- --watch",
    "clean": "rm -rf lib",
    "lint": "standard --verbose {src,test,utils}/**/*.js *.js | snazzy",
    "release": "export NODE_ENV=production && npm run build",
    "prepare": "npm run test && npm run lint && npm run release",
    "postpublish": "npm run clean",
    "seq-gen": "babel-node utils/seq-gen.js"
  },
  "keywords": [
    "gdb",
    "debug",
    "gdbmi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baygeldin/gdb-js.git"
  },
  "bugs": {
    "url": "https://github.com/baygeldin/gdb-js/issues"
  },
  "homepage": "https://github.com/baygeldin/gdb-js#readme",
  "author": "Alexander Baygeldin",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.1",
    "source-map-support": "^0.4.0",
    "stream-splitter": "^0.3.2"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}

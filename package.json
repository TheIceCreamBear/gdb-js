{
  "name": "gdb-js",
  "version": "0.0.1",
  "description": "JavaScript interface for GDB.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run test && npm run standard && npm run release",
    "postpublish": "npm run clean",
    "postinstall": "npm run build && npm run parser-gen",
    "test": "npm run build && npm run mocha",
    "docs": "jsdoc -c jsdoc.json",
    "light-test": "npm run compile && npm run mocha",
    "mocha": "DEBUG=gdb-js:* mocha",
    "parser-gen": "pegjs src/gdbmi.pegjs lib/mi-parser.js && pegjs src/info.pegjs lib/info-parser.js",
    "scripts-concat": "babel-node utils/concat.js scripts lib/scripts.json",
    "compile": "babel src/ -d lib --source-maps --copy-files",
    "build": "npm run compile && npm run parser-gen && npm run scripts-concat",
    "watch": "npm run build -- --watch",
    "clean": "rm -rf lib",
    "standard": "standard --verbose {src,test,utils}/**/*.js *.js",
    "lint": "npm run standard | snazzy",
    "release": "export NODE_ENV=production && npm run build"
  },
  "keywords": [
    "gdb",
    "debug",
    "gdbmi",
    "gdb-mi",
    "inspector"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baygeldin/gdb-js.git"
  },
  "bugs": {
    "url": "https://github.com/baygeldin/gdb-js/issues"
  },
  "homepage": "https://github.com/baygeldin/gdb-js#readme",
  "author": "Alexander Baygeldin",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "docdash": "^0.4.0",
    "dockerode": "^2.2.10",
    "dockerode-promise": "^0.1.0",
    "jsdoc": "^3.4.0",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^2.5.1",
    "pegjs": "^0.9.0",
    "source-map-support": "^0.4.0",
    "standard": "^7.1.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "highland": "^2.8.1"
  }
}
